{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "api/*.js": {
            "runtime": "@vercel/node@3.0.0",
            "maxDuration": 15
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        }
    ]
}
